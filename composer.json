{
    "name": "santino83/composer-monorepo-plugin",
    "license": "MIT",
    "type": "composer-plugin",
    "require": {
        "composer-plugin-api": "^1.0",
        "jawira/case-converter": "^3.0",
        "ext-json": "^1.6"
    },
    "require-dev": {
        "php": "^5.5|^7.0",
        "composer/composer": "^1.1",
        "phake/phake": "^2.2",
        "phpunit/phpunit": "~4.8"
    },
    "autoload": {
        "psr-0": {"Monorepo\\": "src/main"}
    },
    "extra": {
        "class": "Monorepo\\Composer\\Plugin\\MonorepoPlugin",
        "branch-alias": {
            "dev-master": "v1.0-dev"
        }
    }
}
